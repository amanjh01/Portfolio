{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "compat-api/html": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "overscroll-behavior",
          "overscroll-behavior-inline"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off"
  }
}